import pygame

class playerFish(pygame.sprite.Sprite):
    def __init__(self, gs):
        self.image = pygame.image.load("playerFish.png")
        self.image = pygame.transform.scale(self.image, (60, 60))
        self.rect = self.image.get_rect()
        self.rect.x = 50
        self.rect.y = 50
    def move(self, xc, yc):
        self.rect.x += xy
        self.rect.y += yc
        
    def tick(self):
        pass
    
